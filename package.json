{
  "name": "@smpx/cfg",
  "version": "1.1.2",
  "description": "Configuration management for node.js",
  "main": "index.js",
  "browser": {
    "./index.js": "./browser.js"
  },
  "scripts": {
    "build": "true",
    "lint": "eslint index.js browser.js",
    "test": "npm run lint && npm run test-only",
    "test-only": "NODE_ENV=test mocha --slow 0 --exit test/*.test.js",
    "compile": "tsc",
    "prepublish": "yarn lint && yarn build && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprix/cfg.git"
  },
  "keywords": [
    "cfg",
    "config",
    "smpx",
    "smartprix"
  ],
  "author": "Hitesh Khandelwal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprix/cfg/issues"
  },
  "homepage": "https://github.com/smartprix/cfg#readme",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "eslint-config-smartprix": "^3.4.2",
    "mocha": "^5.2.0"
  },
  "eslintConfig": {
    "extends": "smartprix",
    "env": {
      "node": true,
      "browser": false
    }
  }
}
